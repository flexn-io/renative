              "web",
              "ios",
              "android",
              "androidtv",
              "firetv",
              "tvos",
              "macos",
              "linux",
              "windows",
              "tizen",
              "webos",
              "chromecast",
              "kaios",
              "webtv",
              "androidwear",
              "tizenwatch",
              "tizenmobile",
              "xbox"
