{
    "name": "@rnv/engine-rn-next",
    "version": "0.36.0-canary.3",
    "description": "ReNative Engine to build next based platforms with react native support.",
    "keywords": [
        "nextjs",
        "react-native"
    ],
    "homepage": "https://github.com/renative-org/renative#readme",
    "bugs": {
        "url": "https://github.com/renative-org/renative/issues"
    },
    "license": "MIT",
    "author": "Pavel Jacko <pavel.jacko@gmail.com> (https://github.com/pavjacko)",
    "contributors": [],
    "files": [
        "dist",
        "renative.engine.json",
        "supportFiles",
        "templates"
    ],
    "main": "dist/index.js",
    "repository": {
        "type": "git",
        "url": "git://github.com/renative-org/renative.git"
    },
    "scripts": {
        "build": "babel src -d dist --source-maps",
        "build:clean": "rm -rf dist || true",
        "build:dev": "yarn build --retain-lines",
        "build:prod": "babel src -d dist",
        "prepublishOnly": "yarn build:clean && yarn build:prod",
        "watch": "nodemon --watch src --exec yarn build:dev"
    },
    "dependencies": {
        "@expo/next-adapter": "3.1.12",
        "ally.js": "1.4.1",
        "better-opn": "1.0.0",
        "next-fonts": "1.5.1",
        "next-images": "1.8.4",
        "next-transpile-modules": "9.0.0",
        "webpack": "5.65.0"
    },
    "devDependencies": {
        "@babel/cli": "7.17.6",
        "@babel/core": "7.8.3",
        "@babel/node": "7.8.3",
        "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
        "@babel/plugin-proposal-optional-chaining": "7.8.3",
        "@babel/preset-env": "7.8.7"
    },
    "peerDependencies": {
        "next": ">=12.0.8",
        "rnv": "^0.36.0-canary.3"
    },
    "private": false,
    "publishConfig": {
        "access": "public"
    }
}