{
    "name": "@rnv/engine-rn-electron",
    "version": "0.36.0-canary.8",
    "description": "ReNative Engine to build electron based platforms with react native support.",
    "keywords": [
        "electron",
        "react-native"
    ],
    "homepage": "https://github.com/renative-org/renative#readme",
    "bugs": {
        "url": "https://github.com/renative-org/renative/issues"
    },
    "license": "MIT",
    "author": "Pavel Jacko <pavel.jacko@gmail.com> (https://github.com/pavjacko)",
    "contributors": [],
    "files": [
        "dist",
        "renative.engine.json",
        "supportFiles",
        "templates"
    ],
    "main": "dist/index.js",
    "repository": {
        "type": "git",
        "url": "git://github.com/renative-org/renative.git"
    },
    "scripts": {
        "build": "babel src -d dist --source-maps",
        "build:clean": "rm -rf dist || true",
        "build:dev": "yarn build --retain-lines",
        "build:prod": "babel src -d dist",
        "prepublishOnly": "yarn build:clean && yarn build:prod",
        "watch": "nodemon --watch src --exec yarn build:dev"
    },
    "dependencies": {
        "@babel/preset-react": "7.16.0",
        "@rnv/sdk-webpack": "0.36.0-canary.8",
        "babel-loader": "8.0.5",
        "babel-preset-expo": "9.0.2",
        "better-opn": "1.0.0",
        "electron": "11.5.0",
        "electron-builder": "22.13.1",
        "electron-notarize": "1.0.0",
        "html-webpack-harddisk-plugin": "1.0.2",
        "html-webpack-plugin": "4.5.1",
        "webpack-bundle-analyzer": "3.5.1"
    },
    "devDependencies": {
        "@babel/cli": "7.17.6",
        "@babel/core": "7.8.3",
        "@babel/node": "7.8.3",
        "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
        "@babel/plugin-proposal-optional-chaining": "7.8.3",
        "@babel/preset-env": "7.8.7"
    },
    "peerDependencies": {
        "rnv": "^0.36.0-canary.8"
    },
    "private": false,
    "publishConfig": {
        "access": "public"
    }
}
