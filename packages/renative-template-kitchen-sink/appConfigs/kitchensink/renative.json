{
    "id": "kitchensink",
    "common": {
        "id": "renative.kitchensink",
        "title": "ReNative Kitchen Sink",
        "description": "Cross-platform application project based on ReNative",
        "author": "Pavel Jacko <pavel.jacko@gmail.com> (https://github.com/pavjacko)",
        "license": "MIT",
        "includedPlugins": ["*"],
        "includedFonts": ["*"]
    },
    "platforms": {
        "ios": {
            "entryFile": "index.ios",
            "scheme": "RNVApp",
            "teamID": "",
            "permissions": ["*"],
            "buildSchemes": {
                "debug": {
                    "runScheme": "Debug",
                    "bundleAssets": false
                },
                "release": {
                    "runScheme": "Release",
                    "bundleAssets": true,
                    "bundleIsDev": false
                }
            }
        },
        "tvos": {
            "entryFile": "index.tvos",
            "teamID": "",
            "scheme": "RNVAppTVOS",
            "buildSchemes": {
                "debug": {
                    "runScheme": "Debug",
                    "bundleAssets": false
                },
                "release": {
                    "scheme": "RNVAppTVOSRelease",
                    "runScheme": "Release",
                    "bundleAssets": true,
                    "bundleIsDev": false
                }
            }
        },
        "android": {
            "entryFile": "index.android",
            "reactNativeEngine": "hermes",
            "permissions": ["*"],
            "buildSchemes": {
                "debug": {
                    "signingConfig": "Debug",
                    "bundleAssets": false
                },
                "release": {
                    "signingConfig": "Release",
                    "bundleAssets": true,
                    "bundleIsDev": true
                }
            }
        },
        "androidtv": {
            "entryFile": "index.androidtv",
            "reactNativeEngine": "hermes",
            "permissions": ["*"],
            "buildSchemes": {
                "debug": {
                    "signingConfig": "Debug",
                    "bundleAssets": false
                },
                "release": {
                    "signingConfig": "Release",
                    "bundleAssets": true,
                    "bundleIsDev": false
                }
            }
        },
        "firetv": {
            "entryFile": "index.firetv",
            "reactNativeEngine": "hermes",
            "permissions": ["*"],
            "buildSchemes": {
                "debug": {
                    "signingConfig": "Debug",
                    "bundleAssets": false
                },
                "release": {
                    "signingConfig": "Release",
                    "bundleAssets": true,
                    "bundleIsDev": false
                }
            }
        },
        "androidwear": {
            "entryFile": "index.androidwear",
            "reactNativeEngine": "hermes",
            "permissions": ["*"],
            "buildSchemes": {
                "debug": {
                    "signingConfig": "Debug",
                    "bundleAssets": false
                },
                "release": {
                    "signingConfig": "Release",
                    "bundleAssets": true,
                    "bundleIsDev": false
                }
            }
        },
        "macos": {
            "entryFile": "index.macos",
            "buildSchemes": {
                "debug": {
                    "bundleAssets": false,
                    "environment": "development"
                },
                "release": {
                    "bundleAssets": true,
                    "bundleIsDev": false,
                    "environment": "production"
                }
            }
        },
        "tizen": {
            "appName": "RNVanillaTV",
            "package": "NkVRhWHJSU",
            "id": "NkVRhWHJSU.RNVanillaTV",
            "entryFile": "index.tizen",
            "certificateProfile": "RNVanillaCert"
        },
        "tizenwatch": {
            "appName": "RNVanillaWatch",
            "package": "cHIP2fIRQY",
            "id": "cHIP2fIRQY.RNVanillaWatch",
            "entryFile": "index.tizenwatch",
            "certificateProfile": "RNVanillaCert"
        },
        "tizenmobile": {
            "appName": "RenativeMobile",
            "package": "PauodvCU2r",
            "id": "PauodvCU2r.ReNativeMobile",
            "entryFile": "index.tizenwatch",
            "certificateProfile": "RNVanillaCert"
        },
        "web": {
            "entryFile": "index.web",
            "buildSchemes": {
                "debug": {
                    "bundleAssets": false,
                    "environment": "development"
                },
                "release": {
                    "bundleAssets": true,
                    "environment": "production"
                }
            }
        },
        "webtv": {
            "entryFile": "index.webtv",
            "buildSchemes": {
                "debug": {
                    "bundleAssets": false,
                    "environment": "development"
                },
                "release": {
                    "bundleAssets": true,
                    "environment": "production"
                }
            }
        },
        "webos": {
            "entryFile": "index.webos"
        },
        "windows": {
            "entryFile": "index.windows"
        },
        "kaios": {
            "entryFile": "index.kaios",
            "buildSchemes": {
                "debug": {
                    "bundleAssets": false
                },
                "release": {
                    "bundleAssets": true,
                    "bundleIsDev": false
                }
            }
        },
        "firefoxos": {
            "entryFile": "index.firefoxos",
            "buildSchemes": {
                "debug": {
                    "bundleAssets": false
                },
                "release": {
                    "bundleAssets": true,
                    "bundleIsDev": false
                }
            }
        },
        "firefoxtv": {
            "entryFile": "index.firefoxtv",
            "buildSchemes": {
                "debug": {
                    "bundleAssets": false
                },
                "release": {
                    "bundleAssets": true,
                    "bundleIsDev": false
                }
            }
        },
        "chromecast": {
            "entryFile": "index.chromecast",
            "webpackConfig": {
                "customScripts": [
                    "www.gstatic.com/cast/sdk/libs/caf_receiver/v3/cast_receiver_framework.js",
                    "www.gstatic.com/cast/sdk/libs/devtools/debug_layer/caf_receiver_logger.js"
                ]
            }
        }
    }
}
