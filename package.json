        "eslint-plugin-jest": "23.8.2",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "eslint-plugin-mocha": "6.3.0",
        "eslint-plugin-prettier": "3.0.1",
        "eslint-plugin-react": "7.10.0",
        "eslint-plugin-react-hooks": "2.5.0",
        "husky": "4.2.5",
        "jest": "26.4.2",
        "lerna": "4.0.0",
        "lint-staged": "8.1.0",
